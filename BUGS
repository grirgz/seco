Pdef.defaultQuant

(
	
	~plop = { arg x=0;
		x.debug("GN");
		Tdef(\plop).play;
		Tdef(\plop, {
			x.debug("one");
			TempoClock.default.beats.debug("beats");
			1.0.wait;
			x.debug("two");
			TempoClock.default.beats.debug("beats");
			~plop.(x+1);
			TempoClock.default.beats.debug("beats");
			0.wait;
		})

	};
)
